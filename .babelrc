{
  "presets": ["@babel/preset-react", "@babel/preset-env"],
  "plugins": [ 
    [
      "@babel/plugin-transform-runtime", 
      {
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ],
    ["import", { "libraryName": "antd", "libraryDirectory": "lib"}, "antd"],  
    ["import", { "libraryName": "antd", "style": true },"less"]
  ]
}
